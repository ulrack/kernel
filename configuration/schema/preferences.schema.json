{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "preferences.schema.json",
    "type": "object",
    "additionalProperties": {
        "type": "object",
        "properties": {
            "preference": {
                "type": "string",
                "pattern": "^(\\\\[A-z]+)+$"
            },
            "for": {
                "type": "string",
                "pattern": "^(\\\\[A-z]+)+$"
            }
        },
        "required": ["preference", "for"]
    }
}
